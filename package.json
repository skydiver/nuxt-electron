{
  "name": "nuxt-electron",
  "version": "2.0.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently nuxt \"node electron/wait-nuxt.js\" --kill-others",
    "electron-dev": "cross-env NODE_ENV=DEV electron .",
    "electron-build": "nuxt build && electron-builder",
