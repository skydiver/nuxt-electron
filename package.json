    "nuxt-start": "nuxt start",
    "nuxt-generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "clean": "rm -rf .dist .nuxt-build"
