{
  "name": "nuxt-electron",
  "version": "1.0.0",
  "description": "Nuxt.js + Electron starter template",
  "author": "Martín M.",
  "license": "MIT",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently nuxt \"node electron/wait-nuxt.js\"",
    "electron-dev": "cross-env NODE_ENV=DEV electron .",
    "electron-build": "nuxt build && electron-builder",
    "nuxt-dev": "nuxt",
    "nuxt-build": "nuxt build",
    "nuxt-start": "nuxt start",
    "nuxt-generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "clean": "rm -rf .dist .nuxt-build"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "cross-env": "^5.2.0",
    "electron-serve": "^0.3.0",
    "nuxt": "^2.8.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "concurrently": "^4.1.1",
    "electron": "^5.0.6",
    "electron-builder": "^21.0.15",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": ">=13.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-jest": ">=22.7.2",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  }
}
